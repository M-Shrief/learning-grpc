{
  "name": "learning-grpc",
  "version": "0.0.1",
  "description": "Trying out gRPC",
  "main": "index.js",
  "scripts": {
    "pb:generate": "proto-loader-gen-types --longs=String --enums=String --defaults --keepCase --oneofs --grpcLib=@grpc/grpc-js --outDir=pb/ proto/*.proto",
